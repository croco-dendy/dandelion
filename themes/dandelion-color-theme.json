	"tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#616060"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#FDAD00"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    // Typescript
    // {
    //   "name": "Keyword",
    //   "scope": "keyword",
    //   "settings": {
    //     "foreground": "#FDAD00"
    //   }
    // },
    {
      "name": "Import",
      "scope": [
        "keyword.control.import",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#FDAD00"
      }
    },
    {
      "name": "Default and from",
      "scope": [
        "keyword.control.default",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#F27405",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage type",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#5FD7EC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage type async",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
      ],
      "settings": {
        "foreground": "#22D7AA"
      }
    },
    {
      "name": "This",
      "scope": [
        "variable.language.this",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#00FFBF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage module type async",
      "scope": [
        "entity.name.type.module",
      ],
      "settings": {
        "foreground": "#21A795"
      }
    },
    {
      "name": "Type primitive",
      "scope": [
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#21A795",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage type async",
      "scope": [
        "storage.modifier.async",
        "keyword.operator.new",
      ],
      "settings": {
        "foreground": "#626BFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#626BFF",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "null",
      "scope": [
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#656565",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "Constant",
        "storage.type.function.arrow",
        "storage.type.function",
        "keyword.control.flow",
        "keyword.control.conditional",
      ],
      "settings": {
        "foreground": "#EB3C6C",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Keyword comparison",
      "scope": [
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#EB3C6C"
      }
    },
    {
      "name": "True",
      "scope": [
        "constant.language.boolean.true"
      ],
      "settings": {
        "foreground": "#7FDA3C",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "False",
      "scope": [
        "constant.language.boolean.false"
      ],
      "settings": {
        "foreground": "#FF1D57",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#616060"
      }
    },
    {
      "name": "Punctuation Parameters",
      "scope": [
        "punctuation.definition.parameters",
        "keyword.operator.type.annotation",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#616060"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "entity.name.variable.parameter",
        "meta.at-rule.function variable",
        "meta.at-rule.mixin variable",
        "meta.function.arguments",
        "meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#F27405",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        // "keyword.operator.assignment",
        "keyword.operator.logical",
        "keyword.operator.type"
      ],
      "settings": {
        "foreground": "#FDAD00",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Punctuation Template Expression",
      "scope": [
        "punctuation.definition.template-expression",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#FDAD00"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#8df"
      }
    },
    {
      "name": "Variable parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#EB3C6C",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Variable object",
      "scope": [
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#00D8FF"
      }
    },
    {
      "name": "Variable object property",
      "scope": ["variable.other.object.property"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Variable constant",
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "String",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#42C251"
      }
    },
    {
      "name": "String Template",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#42C251"
      }
    },
    {
      "name": "Double string",
      "scope": ["string.quoted.double.tsx"],
      "settings": {
        "foreground": "#42C251"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "Support Function",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "[JAVASCRIPT JSX] React Class",
      "scope": ["support.class.component.js"],
